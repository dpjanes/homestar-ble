      "iot:sensor": true,
      "iot:read": true,
      "iot:write": false,
      "iot:actuator": false
    },
    {
      "@type": "iot:Attribute",
      "@id": "#rssi",
      "schema:name": "rssi",
      "iot:purpose": "iot-purpose:rssi",
      "iot:unit": "iot-unit:math.magnitude.decibel",
      "iot:type": "iot:type.integer",
      "iot:sensor": true,
      "iot:read": true,
      "iot:write": false,
      "iot:actuator": false
    }
  ]
}
